.search-container {
  padding: 16rpx;
}
.search-container .header {
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.0785);
}
.search-container .header .text {
  font-size: 18px;
  font-weight: 700;
  color: rgba(0, 0, 0, 0.8);
  line-height: 1.4;
  position: relative;
}
.search-container .header .text:after {
  position: absolute;
  bottom: -1px;
  left: 0;
  right: 0;
  height: 1px;
  background: #2f2f2d;
  content: '';
}
.search-container .search {
  margin-top: 24rpx;
}
.search-container .search .box {
  position: relative;
}
.search-container .search .box .txt {
  -webkit-appearance: none;
  width: 100%;
  display: block;
  font-size: 14px;
  line-height: 40px;
  height: 40px;
  padding: 0 40px 0 18px;
  box-sizing: border-box;
  color: #313131;
  border: 1px solid rgba(184, 197, 214, 0.2);
  border-radius: 2px;
  outline: 0;
  background-color: #fff;
  box-shadow: 0 1px 4px rgba(0, 0, 0, 0.04);
  padding-left: 93px;
}
.search-container .search .box .txt:hover {
  transition: box-shadow 0.3s;
  box-shadow: 0 1px 4px rgba(0, 0, 0, 0.14);
}
.search-container .search .box .btn {
  position: absolute;
  z-index: 2;
  right: 5px;
  top: 5px;
  width: 30px;
  height: 30px;
}
.search-container .search .box .btn .img {
  position: absolute;
  width: 20px;
  height: 20px;
  left: 5px;
  top: 5px;
}
.search-container .c-dropdown {
  position: absolute;
  left: 0;
  top: 0;
  height: 40px;
  display: inline-block;
  text-align: left;
}
.search-container .c-dropdown__list {
  padding: 6px 0;
  list-style: none;
  position: absolute;
  top: 100%;
  left: 0;
  width: 100%;
  opacity: 0;
  visibility: hidden;
  border-radius: 0 0 3px 3px;
  background: #fff;
  -webkit-transition: opacity 0.25s linear, top 0.25s linear;
  transition: opacity 0.25s linear, top 0.25s linear;
  border: 1px solid #4abdcc;
  border-top: 0;
  box-sizing: border-box;
  z-index: 999;
}
.search-container .c-dropdown.is-open .c-dropdown__list {
  opacity: 1;
  visibility: visible;
  top: 100%;
}
.search-container .c-dropdown__item {
  padding: 6px 12px;
  font-size: 14px;
  line-height: 20px;
  cursor: pointer;
  color: #434A54;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-transition: background-color 0.25s linear;
  transition: background-color 0.25s linear;
}
.search-container .c-dropdown__item:hover {
  background-color: #E6E9ED;
}
.search-container .c-button {
  -webkit-appearance: none;
  margin: 0;
  border: 0;
  overflow: visible;
  font: inherit;
  text-transform: none;
  display: inline-block;
  vertical-align: middle;
  text-decoration: none;
  text-align: center;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
}
.search-container .c-button:hover,
.search-container .c-button:focus {
  outline: none;
  text-decoration: none;
}
.search-container .c-button:not(:disabled) {
  cursor: pointer;
}
.search-container .c-button--dropdown {
  background-color: #4abdcc;
  color: #E6E9ED;
  font-size: 14px;
  height: 40px;
  min-width: 90px;
  line-height: 40px;
  text-align: left;
  border-radius: 2px 0 0 2px;
  text-indent: 10px;
  -webkit-transition: background-color 0.2s linear;
  transition: background-color 0.2s linear;
}
.search-container .c-button--dropdown:hover {
  background-color: #AC92EC;
}
.search-container .c-button--dropdown:after {
  content: "";
  position: absolute;
  top: 17px;
  right: 9px;
  width: 0;
  height: 0;
  border: 5px solid transparent;
  border-top-color: #E6E9ED;
}
.loading {
  color: #b0b0b0;
  text-align: center;
  margin: 20rpx 0;
}
.header-nav {
  position: fixed;
  height: 31px;
  width: 100%;
  left: 0;
  top: 0;
  z-index: 2;
  background: #fff;
  overflow-x: auto;
  display: flex;
  text-align: center;
  justify-content: space-between;
}
.header-nav .nav {
  font-size: 12px;
  font-weight: bold;
  color: #999999;
  cursor: pointer;
  height: 30px;
  line-height: 30px;
  flex: 1;
  text-align: center;
}
.header-nav .nav.active {
  border-bottom: 1px solid #000000;
  color: #1d1f23;
}
page {
  background-color: #fff;
}
/*lower-threshold 生效*/
scroll-view {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  top: 31px;
}
.article-main-box {
  display: flex;
  flex-wrap: wrap;
  padding: 10rpx 15rpx 0;
  /*image text*/
}
.article-main-box .article-container {
  flex: 1;
  position: relative;
  width: 100%;
}
.article-main-box .article-item {
  position: relative;
  margin-bottom: 15rpx;
  box-shadow: 0 0 20rpx rgba(0, 0, 0, 0.15);
  background-color: #fff;
  transition: box-shadow 0.3s;
  line-height: 1.4;
}
.article-main-box .pic {
  position: relative;
  width: 100%;
  height: auto;
}
.article-main-box .pic .article-img {
  width: 100%;
  background-image: linear-gradient(-180deg, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.2) 100%);
}
.article-main-box .publish-date {
  width: 50px;
  height: 50px;
  border-radius: 25px;
  background: rgba(64, 84, 90, 0.7);
  color: #Fff;
  position: absolute;
  top: 10rpx;
  right: 10rpx;
  text-align: center;
  line-height: 1;
}
.article-main-box .publish-date .time {
  display: block;
}
.article-main-box .publish-date .top {
  margin-top: 5px;
}
.article-main-box .title {
  padding: 15px;
  color: #242f35;
  font-size: 16px;
  font-weight: 400;
  /*单行打点*/
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.article-main-box .tips {
  /*多行打点 不用给height*/
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  word-break: break-all;
  line-height: 1.4;
  padding: 0 15px;
  font-size: 14px;
  color: #666;
}
.article-main-box .tag {
  padding: 6px;
  font-size: 14px;
  color: rgba(255, 255, 255, 0.8);
  background: rgba(0, 0, 0, 0.7);
  border-radius: 4px;
  display: inline-block;
  margin: 8px 0 8px 15px;
  line-height: 1;
}
.article-main-box .info {
  display: inline-block;
  line-height: 1;
  padding: 0 10px;
  font-size: 14px;
  color: #666;
  opacity: 0.98;
}
.article-main-box .info image {
  width: 20px;
  height: 20px;
  vertical-align: middle;
}
.article-main-box .info text {
  padding: 4px;
}
/* 返回顶部 */
.widget-goTop {
  position: fixed;
  bottom: 35px;
  right: 15px;
  background: rgba(0, 0, 0, 0.48);
  border-radius: 50%;
  overflow: hidden;
  z-index: 500;
}
.widget-goTop .gotop-wrap {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 50px;
  height: 50px;
  font-size: 12px;
  color: #fff;
  border-radius: 50%;
  background-color: rgba(0, 0, 0, 0.8);
  -webkit-background-size: 50px auto;
}
.result {
  padding: 16rpx;
}
.result .inner {
  text-align: left;
  padding-bottom: 6px;
  font-size: 16px;
  height: 30px;
  line-height: 30px;
  color: #242f35;
  border-left: 4px solid #4abdcc;
  padding-left: 4px;
}
.result text {
  color: #1da496;
  margin: 0 5px;
  font-weight: 500;
}
/*lower-threshold 生效*/
scroll-view {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  top: 100px;
}
/*列表*/
.comment-container {
  padding: 16rpx;
}
.comment-container .comment-total {
  color: #555;
  font-size: 16px;
}
.comment-container .comment-total .strong {
  font-size: 17px;
  margin-right: 4px;
  font-weight: 600;
}
.comment-container .comment-list {
  margin-top: 20px;
}
.comment-container .comment-list .comment-item {
  position: relative;
}
.comment-container .comment-list .comment-item .avatar {
  width: 2.5em;
  height: 2.5em;
  float: left;
  border: 1px solid #f5f5f5;
  margin-right: 0.7525em;
  padding: 0.125em;
}
.comment-container .comment-list .comment-item .info {
  overflow: hidden;
  /*bfc*/
  padding-bottom: 0.5em;
  border-bottom: 1px dashed #f5f5f5;
}
.comment-container .comment-list .comment-item .info .head {
  line-height: 1.5;
  font-size: 0.875em;
  font-weight: 500;
  color: #1da496;
}
.comment-container .comment-list .comment-item .info .reply-time,
.comment-container .comment-list .comment-item .info .stars {
  color: #b3b3b3;
  font-size: 0.75em;
  position: relative;
  margin-top: 2px;
}
.comment-container .comment-list .comment-item .info .reply-time .reply,
.comment-container .comment-list .comment-item .info .stars .reply {
  position: absolute;
  right: 0;
  top: 0;
  padding: 4px;
  font-size: 1em;
  color: #ef2f11;
}
.comment-container .comment-list .comment-item .info .content {
  word-wrap: break-word;
  word-break: break-all;
  text-align: justify;
  color: #4a4a4a;
  font-size: 0.875em;
  line-height: 2;
  position: relative;
  margin-bottom: 0.75em;
  padding-top: 0.625em;
}
/*评论*/
.comment-box {
  font-size: 16px;
  padding: 16rpx;
}
.comment-box .wrap {
  border: 1px solid #f0f0f0;
  border-radius: 4px;
  margin-bottom: 10px;
  overflow: hidden;
  position: relative;
  padding: 10px;
}
.comment-box .header {
  box-sizing: border-box;
  line-height: 2;
  color: #555;
}
.comment-box .header .vinput {
  border: none;
  resize: none;
  outline: none;
  padding: 10px 5px;
  max-width: 100%;
  font-size: 0.775em;
  border-bottom: 1px dashed #dedede;
}
.comment-box .vedit {
  position: relative;
  padding-top: 10px;
}
.comment-box .vedit .veditor {
  width: 100%;
  display: block;
  padding-top: 12px;
  min-height: 8.75em;
  font-size: 0.875em;
  background: transparent;
  resize: vertical;
  transition: all 0.25s ease;
  line-height: 2;
}
.comment-box .vctrl {
  font-size: 12px;
  text-align: right;
  box-sizing: border-box;
  line-height: 2;
  color: #555;
}
.comment-box .vctrl .vemoji-btn {
  padding: 10px;
  display: inline-block;
  vertical-align: middle;
  cursor: pointer;
}
.comment-box .vemojis {
  display: -none;
  font-size: 18px;
  text-align: justify;
  max-height: 145px;
  overflow-y: auto;
  margin-bottom: 10px;
  -webkit-box-shadow: 0 0 1px #f0f0f0;
  box-shadow: 0 0 1px #f0f0f0;
}
.comment-box .vemojis text {
  font-style: normal;
  padding: 7px 0;
  width: 38px;
  cursor: pointer;
  text-align: center;
  display: inline-block;
  vertical-align: middle;
}
.comment-box .vcontrol {
  -webkit-transition-duration: 0.4s;
  transition-duration: 0.4s;
  text-align: center;
  color: #313131;
  border: 1px solid #ededed;
  border-radius: 0.3em;
  display: inline-block;
  background: #ededed;
  margin-bottom: 0;
  font-weight: 400;
  vertical-align: middle;
  -ms-touch-action: manipulation;
  touch-action: manipulation;
  cursor: pointer;
  white-space: nowrap;
  padding: 0.5em 1.25em;
  font-size: 0.875em;
  line-height: 1.42857143;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  outline: none;
  float: right;
  margin-top: 4px;
}
